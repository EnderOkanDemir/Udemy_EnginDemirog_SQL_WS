Select LOWER('EnDeR')				ender							
Select UPPER('EnDeR')				ENDER							
											
SELECT SUBSTRING('ender', 1, 1) AS first_letter						             	 e					
SELECT SUBSTRING('ender', 1, 3) AS first_three_letter						        end					
SELECT SUBSTRING('ender', 2, 3) AS first_three_letter						        nde					
SELECT UPPER(SUBSTRING('ender', 2, 3)) AS first_three_letter						NDE					
